function Mobil(tipe, nama, harga){
  this.nama = nama;
  this.tipe = tipe;
  this.harga = harga;
  
  this.pergi = function(tempat){
    return `Pergi ke ${tempat} dengan ${this.nama}`;
  }
}

let mobil1 = new Mobil("Angkot", "32", 5000);
console.log(mobil1.nama);
console.log(mobil1.pergi("Bogor"));